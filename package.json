  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack-dev-server --config ./webpack.dev.js",
    "dev:server": "LOG_LEVEL=debug ts-node-dev --no-notify --files --require tsconfig-paths/register ./server/index.ts",
    "dev:nodemon": "LOG_LEVEL=debug nodemon",
    "build:client": "CONFIG=production NODE_ENV=production webpack --config ./webpack.prod.js",
    "build:server": "CONFIG=production NODE_ENV=production webpack --config ./webpack.server.js",
    "build": "run-s build:server build:client",
    "deploy": "CONFIG=production NODE_ENV=production shipit production deploy",
    "prod": "CONFIG=production NODE_ENV=production node ./build/server.js",
    "prod:raw": "TS_NODE_FILES=true NODE_ENV=production CONFIG=production npx ts-node --project tsconfig.json -r tsconfig-paths/register server/index.ts",
    "forever": "LOG_LEVEL=debug CONFIG=production NODE_ENV=production forever start ./forever.json",
    "db:sync": "node ./db/sync.js",
    "db:migrate": "sequelize db:migrate",
    "log": "tail -f -n 100 ./tmp/output.log | bunyan",
    "lint:client": "tslint -p ./client/tsconfig.json -c ./client/tslint.json -t stylish --fix",
    "lint:server": "tslint -p ./tsconfig.json -c ./tslint.json -t stylish --fix",
    "lint:es": "eslint --fix",
    "ts": "ts-node --project tsconfig.json -r tsconfig-paths/register --"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.4"
  }
